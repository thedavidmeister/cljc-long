(ns cljc-long.comparison-test
 (:require
  [cljc-long.core :as l]
  [clojure.test :refer [deftest is]]))

(deftest ??>
 ; (> 9007199254740993 9007199254740992)
 ; false in cljs
 (is
  (l/>
   (l/long "9007199254740993")
   (l/long "9007199254740992")))

 (is
  (l/>
   (l/long "9007199254740993")
   (l/long "9007199254740992")
   (l/long "9007199254740991")
   (l/long "-9007199254740990")))

 (is
  (not
   (l/>
    (l/long "9007199254740993")
    (l/long "9007199254740992")
    (l/long "9007199254740991")
    (l/long "9007199254740991")))))

(deftest ??>=
 (is
  (l/>=
   (l/long "9007199254740993")
   (l/long "9007199254740992")))

 (is
  (l/>=
   (l/long "9007199254740993")
   (l/long "9007199254740992")
   (l/long "9007199254740992")
   (l/long "9007199254740991")))

 (is
  (not
   (l/>=
    (l/long "9007199254740993")
    (l/long "9007199254740992")
    (l/long "9007199254740992")
    (l/long "9007199254740993")))))

(deftest ??=
 ; (= 9007199254740992 9007199254740993)
 ; true in cljs
 (is
  (not
   (l/=
    (l/long "9007199254740992")
    (l/long "9007199254740993"))))

 (is
  (not
   (l/=
    (l/long "9007199254740992")
    (l/long "9007199254740992")
    (l/long "9007199254740993"))))

 (is
  (l/=
   (l/long "9007199254740992")
   (l/long "9007199254740992")
   (l/long "9007199254740992"))))

(deftest ??not=
 (is
  (l/not=
   (l/long "9007199254740992")
   (l/long "9007199254740993")))

 (is
  (l/not=
   (l/long "9007199254740992")
   (l/long "9007199254740992")
   (l/long "9007199254740993")))

 (is
  (not
   (l/not=
    (l/long "9007199254740992")
    (l/long "9007199254740992")
    (l/long "9007199254740992")))))

(deftest ??<=
 (is
  (l/<=
   (l/long "9007199254740992")
   (l/long "9007199254740993")))

 (is
  (l/<=
   (l/long "9007199254740992")
   (l/long "9007199254740993")
   (l/long "9007199254740993")
   (l/long "9007199254740994")))

 (is
  (not
   (l/<=
    (l/long "9007199254740992")
    (l/long "9007199254740993")
    (l/long "9007199254740994")
    (l/long "9007199254740993")))))

(deftest ??<
 ; (< 9007199254740992 9007199254740993)
 ; false in cljs
 (is
  (l/<
   (l/long "9007199254740992")
   (l/long "9007199254740993")))

 (is
  (not
   (l/<
    (l/long "9007199254740992")
    (l/long "9007199254740993")
    (l/long "9007199254740993")
    (l/long "9007199254740994"))))

 (is
  (l/<
   (l/long "9007199254740992")
   (l/long "9007199254740993")
   (l/long "9007199254740994"))))

(deftest ??compare
 (is
  (==
   -1
   (l/compare
    (l/long "9007199254740992")
    (l/long "9007199254740993"))))

 (is
  (==
   0
   (l/compare
    (l/long "9007199254740993")
    (l/long "9007199254740993"))))

 (is
  (==
   1
   (l/compare
    (l/long "9007199254740993")
    (l/long "9007199254740992")))))
